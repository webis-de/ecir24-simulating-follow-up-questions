<View>
  <Style>
    .htx-text{
      white-space: pre-wrap;
    }
    h4 {
      font-weight: normal !important;
      font-size: revert !important;
    }
    .unit {
      margin-bottom: 50px;
      border-top: 1px solid;
    }
    .unit .lsf-richtext {
      font-weight: bold;
    }
    .slider {
      display: flex;
      column-gap: 10px;
    }
    .slider h4 {
      width: 8em;
    }
    .slider h4:first-child {
      text-align: right;
    }
  </Style>
 
  <Collapse>
  <Panel>
  <View>
  <Text name="history" value="$history"/>
  </View>
  </Panel>
  </Collapse>
<View className="unit">
<Header value="0. $system"/>
<Text name="user-response0" value="$user_response_0"/>
<Choices name="ratings0" toName="user-response0" choice="multiple" showInline="true">
  <Choice value="valid (complete and comprehensible question)" alias="valid"/>
  <Choice value="related (not off-topic)" alias="related"/>
  <Choice value="generic (fits to all texts)" alias="generic"/>
  <Choice value="informative (not answered in the text)" alias="informative"/>
</Choices>
<View className="slider">
<Header value="naive"/>
<Number name="expertise0" toName="user-response0" min="-1" max="1" slider="true"/>
<Header value="savvy"/>
</View>
<View className="slider">
<Header value="reasons"/>
<Number name="direction0" toName="user-response0" min="-1" max="1" slider="true"/>
<Header value="implications"/>
</View>
<TextArea name="comments0" toName="user-response0" placeholder="comments" />
</View>
